<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.tcmp.dao.CoachMapper">
    <select id="queryCoach" resultType="Coach">
     SELECT * FROM coach
    </select>
    <update id="updateCoach" parameterType="integer">
 UPDATE coach SET coathStatevalueId=3 WHERE coachId=#{coachId}
    </update>
    <update id="updateCoach2" parameterType="integer">
 UPDATE coach SET coathStatevalueId=1 WHERE coachId=#{coachId}
    </update>
</mapper>